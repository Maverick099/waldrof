{
  "name": "launchpad",
  "version": "0.0.1",
  "description": "A launchpad to organize apps.",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "devDependencies": {
    "@sap-ux/eslint-plugin-fiori-tools": "^0.6.0",
    "@sap/ux-ui5-tooling": "^1.13.3",
    "@sapui5/types": "~1.140.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@ui5/cli": "^4.0.16",
    "typescript": "^5.9.2",
    "ui5-tooling-transpile": "^3.3.7"
  },
  "scripts": {
    "start": "fiori run --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#app-tile\"",
    "start:ui5": "ui5 serve --port 8080 -o index.html",
    "build": "ui5 build -a --clean-dest --include-task=generateManifestBundle generateCachebusterInfo",
    "deploy-config": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf",
    "start-noflp": "fiori run --open \"index.html?sap-ui-xx-viewCache=false\""
  }
}
