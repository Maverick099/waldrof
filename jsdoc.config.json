{
  "source": {
    "include": ["srv", "app"],
    "includePattern": "\\.(ts|tsx|cds)$",
    "excludePattern": "(node_modules|test|dist|webapp/resources|webapp/test|\\.test\\.js|\\.spec\\.js)$"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true
    }
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/api",
    "recurse": true,
    "verbose": true
  },
  "jsdoc": {
    "tags": {
      "allowUnknownTags": true
    },
    "typescript": {
      "moduleResolution": "node"
    }
  }
}
